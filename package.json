{
    "name": "sqcr",
    "version": "1.0.3",
    "description": "a command-line sequencer",
    "repository": "omardelarosa/sqcr",
    "author": {
        "name": "Omar Delarosa",
        "email": "thedelarosa@gmail.com",
        "url": "omardelarosa.com"
    },
    "engines": {
        "node": ">=8"
    },
    "bin": {
        "sqcr": "./sqcr-cli.js"
    },
    "main": "index.js",
    "scripts": {
        "test": "echo 'coming soon!'",
        "build":
            "npm run build:worker && npm run build:browser && npm run build:server",
        "build:browser": "webpack --config config/webpack.production.config.js",
        "build:worker": "webpack --config config/webpack.worker.config.js",
        "build:server": "tsc -p src/server/tsconfig.json",
        "start:debug": "./sqcr-cli.js debug"
    },
    "keywords": [
        "cli",
        "midi",
        "util",
        "osc",
        "music",
        "websockets",
        "udp",
        "fun"
    ],
    "license": "MIT",
    "dependencies": {
        "cli-color": "^1.2.0",
        "debug": "^3.1.0",
        "express": "^4.16.3",
        "lodash": "^4.17.10",
        "meow": "^5.0.0",
        "midi-clock": "0.0.1",
        "node-watch": "^0.5.8",
        "osc": "^2.2.3",
        "tonal": "^1.1.3",
        "tone": "^0.12.80",
        "typescript": "^2.9.2",
        "webmidi": "^2.2.0",
        "ws": "^5.2.1"
    },
    "devDependencies": {
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.110",
        "@types/node": "^10.5.1",
        "@types/ws": "^5.1.2",
        "raw-loader": "^0.5.1",
        "ts-loader": "^4.4.2",
        "webpack": "^4.14.0",
        "webpack-cli": "^3.0.8",
        "worker-loader": "^2.0.0"
    }
}
